{
  "name": "@aiobs/platform",
  "version": "1.0.0",
  "description": "AI Observability Hub - Trust Control Layer for AI Systems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/server.ts",
    "start": "node dist/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --reporters=default --reporters=jest-junit",
    "test:continuous": "python scripts/watch_tests.py --interval 60",
    "test:continuous:fast": "python scripts/watch_tests.py --interval 30",
    "test:all": "npm test && python -m pytest tests/ -v",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "docs": "typedoc --out docs/api src",
    "clean": "rimraf dist"
  },
  "keywords": [
    "ai",
    "observability",
    "mlops",
    "governance",
    "compliance",
    "ai-act",
    "drift-detection",
    "hallucination",
    "slo",
    "finops",
    "greenops"
  ],
  "author": "AIOBS Team",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "uuid": "^9.0.0",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.27",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "globals": "^16.5.0",
    "jest": "^29.6.0",
    "jest-junit": "^16.0.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "ts-node-dev": "^2.0.0",
    "typedoc": "^0.25.0",
    "typescript": "^5.2.0",
    "typescript-eslint": "^8.49.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/aiobs.git"
  }
}
